<Window x:Class="PasswordCollector.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Välkommen - Logga in!" Height="269.433" Width="379.732">
    <Grid Margin="0,0,2,-1">
        <Grid Background="#FFE5E5E5">
            <Button x:Name="BtnLogIn" Content="Logga in" HorizontalAlignment="Left" Margin="282,204,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
            <TextBox x:Name="TbUsername" HorizontalAlignment="Left" Height="23" Margin="108,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
            <PasswordBox x:Name="PbPassword" HorizontalAlignment="Left" Height="23" Margin="108,94,0,0" VerticalAlignment="Top" Width="120"/>
            <Label x:Name="LblUsername" Content="Användarnamn:" HorizontalAlignment="Left" Margin="10,63,0,0" VerticalAlignment="Top"/>
            <Label x:Name="LblPassword" Content="Lösenord:" HorizontalAlignment="Left" Margin="10,89,0,0" VerticalAlignment="Top"/>
        </Grid>
    </Grid>
</Window>
